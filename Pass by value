#include<iostream>
using namespace std;

// Pass by value, here the values of a and b are directly copied into the function signature
// 
//
//
//

void passByValue(int a , int b){
    int z=a;
    a=b;
    b=z;
    
}


int main(){
    int a =7;
    int b =6;
    cout << "Before swapping "<<endl;
    cout <<"a: "<<a<<endl;
    cout << "b: "<<b<<endl;
    
    passByValue(a,b);

    cout << "After swapping "<<endl;
    cout <<"a: "<<a<<endl;
    cout << "b: "<<b<<endl;
    

    
    return 0;
}
